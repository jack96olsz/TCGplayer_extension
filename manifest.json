{
    "manifest_version": 2,
    "name": "TCGPlayer Extension",
    "version": "1.0",
    "description": "Extension to add search results to a list",
    "permissions": ["activeTab", "storage"],
    "content_scripts": [
      {
        "matches": ["https://www.tcgplayer.com/*"],
        "js": ["content.js"]
      }
    ],
    "browser_action": {
      "default_icon": {
        "16": "images/icon16.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
      },
      "default_popup": "popup.html"
    },
    "icons": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  }
  